{
  "name": "vtranslit-web",
  "version": "0.3.1",
  "description": "A web app for vtranslit.",
  "author": "Prasanna Venkatesh T S <vipranarayan14@gmail.com>",
  "license": "MIT",
  "keywords": [
    "webapp",
    "vtranslit",
    "ITRANS",
    "Indic scripts"
  ],
  "homepage": "https://github.com/vipranarayan14/vtranslit-web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vipranarayan14/vtranslit-web.git"
  },
  "bugs": {
    "url": "https://github.com/vipranarayan14/vtranslit-web/issues"
  },
  "main": "prod/app.js",
  "scripts": {
    "build": "rimraf dist && webpack --env.target=dist",
    "build:final": "rimraf dist && webpack --env.target=dist && npm run inliner",
    "eslint": "eslint ./**/*.js",
    "inliner": ". ./inliner.sh",
    "postversion": "git push && git push --tags",
    "precommit": "npm run eslint",
    "preversion": "npm run build:final",
    "start": "webpack-dev-server --config webpack.config.server --env.target=dist",
    "version": "git add -A dist"
  },
  "dependencies": {
    "vtranslit": "^4.2.0",
    "vtranslit-scheme-deva": "^0.4.0",
    "vtranslit-scheme-itrn": "^0.3.0",
    "vtranslit-scheme-knda": "^0.3.0",
    "vtranslit-scheme-taml": "^0.3.0",
    "vtranslit-scheme-telu": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.15.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "inliner": "^1.13.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
